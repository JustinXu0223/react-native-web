{
  "name": "react_native_web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "react-native run-android --appIdSuffix=debug --no-packager && adb reverse tcp:8081 tcp:8081",
    "ios": "react-native run-ios",
    "iPhoneX": "react-native run-ios --simulator 'iPhone X'",
    "web": "NODE_ENV=development webpack-dev-server -d --config ./web-build/webpack.config.js --inline --hot --colors",
    "build": "NODE_ENV=production webpack -p --config ./web-build/webpack.config.js",
    "test": "jest",
    "clear": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && yarn install && yarn start -- --reset-cache"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "dependencies": {
    "react": "16.6.3",
    "react-art": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-native": "0.58.5",
    "react-native-web": "^0.10.0",
    "react-navigation": "^3.3.0",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "^8.0.5",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-native": "^3.6.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.52.0",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.7.2",
    "react-test-renderer": "16.6.3",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
